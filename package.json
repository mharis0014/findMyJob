    "lint-fix": "--fix",
    "start": "react-native start",
    "test": "jest",
    "clean": "cd android && ./gradlew clean",
    "doctor": "npx react-native doctor",
    "asset": "npx react-native-asset",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/src/main/assets/index.android.bundle",
    "build-android": "cd android && ./gradlew assembleRelease",
    "build-ios": "xcodebuild -workspace findMyJob.xcworkspace -scheme YourScheme -configuration Release -destination 'generic/platform=iOS' archive -archivePath build/findMyJob.xcarchive && xcodebuild -exportArchive -archivePath build/findMyJob.xcarchive -exportOptionsPlist exportOptions.plist -exportPath build/findMyJob.ipa",
    "analyze-bundle": "react-native-bundle-analyzer",
    "lint:ts": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "type-check": "tsc --noEmit"
